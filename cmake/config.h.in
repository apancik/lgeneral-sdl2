/* Package information */
#define PACKAGE "@PACKAGE@"
#define VERSION "@PROJECT_VERSION@"
#define TOP_SRCDIR "@TOP_SRCDIR@"

/* Native language support */
#cmakedefine ENABLE_NLS 1

/* Header availability */
/* Define to 1 if you have the <string.h> or <strings.h> header. */
#ifndef HAVE_STRING_H
#cmakedefine01 HAVE_STRING_H
#endif
/* Define to 1 if you have <locale.h> */
#ifndef HAVE_LOCALE_H
#cmakedefine01 HAVE_LOCALE_H
#endif
/* Define to 1 if you have <libintl.h> */
#ifndef HAVE_LIBINTL_H
#cmakedefine01 HAVE_LIBINTL_H
#endif

/* Function / symbol availability */
/* Define to 1 if you have the `strcasestr' function. */
#ifndef HAVE_STRCASESTR
#cmakedefine01 HAVE_STRCASESTR
#endif

/* Portability helpers */
/* Define to 1 if setenv() is available */
#ifndef HAVE_SETENV
#cmakedefine01 HAVE_SETENV
#endif
/* Define to 1 if putenv() is available */
#ifndef HAVE_PUTENV
#cmakedefine01 HAVE_PUTENV
#endif

/* Additional helpers ported from Autoconf */

/* If a macro was generated as `0`, undef it so downstream headers that
   define the macro as `1` (for example SDL_config.h) don't trigger
   `-Wmacro-redefined` warnings. Undef guards are applied broadly to
   common HAVE_* checks to reduce accidental redefinition warnings. */

#if defined(HAVE_STRING_H) && (HAVE_STRING_H == 0)
#  undef HAVE_STRING_H
#endif
#if defined(HAVE_LOCALE_H) && (HAVE_LOCALE_H == 0)
#  undef HAVE_LOCALE_H
#endif
#if defined(HAVE_STRCASESTR) && (HAVE_STRCASESTR == 0)
#  undef HAVE_STRCASESTR
#endif
#if defined(HAVE_SETENV) && (HAVE_SETENV == 0)
#  undef HAVE_SETENV
#endif
#if defined(HAVE_PUTENV) && (HAVE_PUTENV == 0)
#  undef HAVE_PUTENV
#endif
